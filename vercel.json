{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python@3.1.0",
            "config": {
                "maxDuration": 30,
                "memory": 10240,
                "runtime": "python3.8"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.py",
            "methods": ["GET", "POST", "OPTIONS"]
        }
    ]
} 