{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.py",
            "methods": ["GET", "POST", "OPTIONS"],
            "continue": true
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
} 